  pod 'Firebase/Auth'
  pod 'SwiftyJSON'
  pod 'Alamofire'
  target 'BaseAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BaseAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
